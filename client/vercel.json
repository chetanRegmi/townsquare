{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*).js",
        "headers": { "Content-Type": "application/javascript" }
      },
      { "src": "/(.*)", "dest": "/" }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }