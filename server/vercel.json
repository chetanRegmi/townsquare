{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "build": {
            "env": {
              "NODE_OPTIONS": "--experimental-modules"
            }
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "build": {
      "env": {
        "NODE_OPTIONS": "--experimental-modules"
      }
    },
    "buildCommand": "node src/seed.js && npm run build"
  }